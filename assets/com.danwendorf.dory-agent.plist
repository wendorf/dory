<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Label</key>
	<string>com.danwendorf.dory-agent</string>
	<key>ProgramArguments</key>
	<array>
		<string>/usr/local/bin/dory</string>
		<string>server</string>
	</array>
	<key>Sockets</key>
	<dict>
		<key>Listeners</key>
		<dict>
			<key>SecureSocketWithKey</key>
			<string>DORY_SOCK</string>
		</dict>
	</dict>
	<key>KeepAlive</key>
	<true/>
	<key>StandardErrorPath</key>
	<string>/usr/local/var/log/dory-agent.log</string>
	<key>StandardOutPath</key>
	<string>/usr/local/var/log/dory-agent.log</string>
</dict>
</plist>
